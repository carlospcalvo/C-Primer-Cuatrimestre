#include <stdio.h>
#include <stdlib.h>

void mostrarNombres(char mat[][20]);

int main()
{
    char nombres [5][20]; //[filas] [columnas]

    for (int i = 0; i < 5; i++){
        printf("Ingrese un nombre: ");
        fflush(stdin);
        gets(nombres[i]);
    }

    for (int i = 0; i < 5; i++){
        printf("\n%s\n", nombres[i]);
    }

    printf("\n\n");

    return 0;
}
